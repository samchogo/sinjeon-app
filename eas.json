{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { 
        "APP_VARIANT": "dev",
        "EXPO_NO_CAPABILITY_SYNC": "1"
       }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": { "APP_VARIANT": "prod" }
    },
    "dev-apk": {
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": { "APP_VARIANT": "dev" }
    },
    "production-dev": {
      "distribution": "store",
      "env": { "APP_VARIANT": "dev" }
    }
  },
  "submit": {
    "production": {},
    "production-dev": {}
  }
}
